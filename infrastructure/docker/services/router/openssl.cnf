[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions	= v3_req

[dn]
C=FR
ST=Paris
L=Paris
O=MyApp
OU=IT
CN=*.myapp.joli

[v3_req]
subjectKeyIdentifier=hash
authorityKeyIdentifier=keyid:always,issuer:always
basicConstraints=CA:true
subjectAltName = @alt_names

[alt_names]
DNS.1 = app.test
DNS.2 = www.app.test
